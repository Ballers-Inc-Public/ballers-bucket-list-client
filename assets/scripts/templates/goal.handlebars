

{{#each goals as |goal|}}


<tr id='table-data-{{goal.id}}'>
  <td>{{goal.id}}</td>
  <td>{{goal.title}}</td>
  <td>{{goal.status}}</td>
  <td><button type="button" id='modify-{{goal.id}}'>
    Modify
  </button></td>
  <td><button type="button" id='delete-{{goal.id}}'>
    Delete
  </button></td>
</tr>

{{/each}}
